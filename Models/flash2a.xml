 <!-- Starter -->
 <animation>
  <type>pick</type>
  <object-name>StarterHandle</object-name>
  <action>
    <name>Start</name>
    <button>0</button>
    <repeatable>true</repeatable>
    <binding>
     <command>nasal</command>
     <script>controls.startEngine()</script>
    </binding>
    <mod-up>
     <binding>
      <command>nasal</command>
      <script>props.setAll("/controls/engines/engine", "starter", 0)</script>
     </binding>
    </mod-up>
  </action>
 </animation>

 <animation>
  <type>select</type>
  <object-name>StarterHandle</object-name>
  <condition>
   <equals>
    <property>engines/engine[0]/cranking</property>
    <value>false</value>
   </equals>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>StarterHandlePulled</object-name>
  <object-name>StarterRope2Pulled</object-name>
  <condition>
   <equals>
    <property>engines/engine[0]/cranking</property>
    <value>true</value>
   </equals>
  </condition>
 </animation>

  <!-- Object movements -->

 <animation>
  <type>rotate</type>
  <object-name>NoseWheel</object-name>
  <object-name>NoseStrut</object-name>
  <property>fdm/jsbsim/fcs/rudder-cmd-norm</property>
  <factor>20</factor>
  <center>
   <x-m>0.24</x-m>
   <y-m>0.0</y-m>
   <z-m>-0.22</z-m>
  </center>
  <axis>
   <x>-0.2</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>

 <animation>
  <type>select</type>
  <object-name>Prop</object-name>
  <condition>
   <less-than>
    <property>engines/engine[0]/rpm</property>
    <value>500</value>
   </less-than>
  </condition>
 </animation>

 <animation>
  <type>spin</type>
  <object-name>Prop</object-name>
  <property>engines/engine[0]/rpm</property>
  <factor>-1</factor>
  <center>
   <x-m>2.4</x-m>
   <y-m>0</y-m>
   <z-m>0.55</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Wing</object-name>
  <object-name>ControlBar</object-name>
  <object-name>ControlBar2</object-name>
  <object-name>Rudder</object-name>
  <object-name>KingPost</object-name>
  <object-name>WingKeel</object-name>
  <object-name>ControlBarStrap</object-name>
  <object-name>ControlBarBolt1</object-name>
  <object-name>ControlBarBolt2</object-name>
  <property>controls/flight/aileron</property>
  <interpolation>
   <entry>
    <ind>-1.0</ind>
    <dep>15</dep>
   </entry>
   <entry>
    <ind>0.0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>1.0</ind>
    <dep>-15</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>1.55</x-m>
   <y-m>0.0</y-m>
   <z-m>1.96</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Wing</object-name>
  <object-name>ControlBar</object-name>
  <object-name>ControlBar2</object-name>
  <object-name>Rudder</object-name>
  <object-name>KingPost</object-name>
  <object-name>WingKeel</object-name>
  <object-name>ControlBarStrap</object-name>
  <object-name>ControlBarBolt1</object-name>
  <object-name>ControlBarBolt2</object-name>
  <object-name>HangStrap</object-name>
  <property>controls/flight/elevator</property>
  <interpolation>
   <entry>
    <ind>-1.0</ind>
    <dep>-6</dep>
   </entry>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>1.0</ind>
    <dep>12</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>1.55</x-m>
   <y-m>0.0</y-m>
   <z-m>1.96</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 
</PropertyList>
