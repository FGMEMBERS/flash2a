<?xml version="1.0"?>

<PropertyList>


 <path>flash2a.ac</path>

 <!-- 3D Instruments -->
 <model>
  <name>ASI</name>
  <path>Aircraft/flash2a/Models/asi/asi.xml</path>
  <offsets>
   <x-m>0.66</x-m>
   <y-m>-0.14</y-m>
   <z-m>0.32</z-m>
   <pitch-deg>-38</pitch-deg>
  </offsets>
 </model>

 <model>
  <name>Compass</name>
  <path>Aircraft/flash2a/Models/compass/compass.xml</path>
  <offsets>
   <x-m>0.67</x-m>
   <y-m>0.0</y-m>
   <z-m>0.305</z-m>
  </offsets>
 </model>

 <model>
  <name>Altimeter</name>
  <path>Aircraft/flash2a/Models/alt/alt.xml</path>
  <offsets>
   <x-m>0.64</x-m>
   <y-m>-0.06</y-m>
   <z-m>0.345</z-m>
   <pitch-deg>-38</pitch-deg>
  </offsets>
 </model>

 <model>
  <name>EGT</name>
  <path>Aircraft/flash2a/Models/egt/egt.xml</path>
  <offsets>
   <x-m>0.64</x-m>
   <y-m>0.09</y-m>
   <z-m>0.35</z-m>
   <pitch-deg>-38</pitch-deg>
  </offsets>
 </model>

 <model>
  <name>RPM</name>
  <path>Aircraft/flash2a/Models/rpm/rpm.xml</path>
  <offsets>
   <x-m>0.665</x-m>
   <y-m>0.14</y-m>
   <z-m>0.33</z-m>
   <pitch-deg>-38</pitch-deg>
  </offsets>
 </model>

<!-- Transparent Surfaces for Rembrandt rendering -->
 <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>Visor</object-name>
 </effect>

  
<!-- Hand Throttle -->
  <animation>
  <type>rotate</type>
  <object-name>HandThrottle</object-name>
   <property>controls/engines/engine[0]/handthrottle</property>
  <factor>270</factor>
  <axis>
      <x1-m>1.33</x1-m>
      <y1-m>-0.342</y1-m>
      <z1-m>0.227</z1-m>
      <x2-m>1.329</x2-m>
      <y2-m>-0.32</y2-m>
      <z2-m>0.226</z2-m>
    </axis>
 </animation>

