 <!-- Magneto Switch -->
 <animation>
  <type>pick</type>
  <object-name>MagnetoSwitch_Off</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-assign</command>
      <property>/controls/engines/engine/magnetos</property>
      <value>0</value>
    </binding>
  </action>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>MagnetoSwitch_On</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-assign</command>
      <property>/controls/engines/engine/magnetos</property>
      <value>1</value>
    </binding>
  </action>
 </animation>

 <!-- Starter -->
 <animation>
  <type>pick</type>
  <object-name>StarterHandle</object-name>
  <action>
    <name>Start</name>
    <button>0</button>
    <repeatable>true</repeatable>
    <binding>
     <command>nasal</command>
     <script>controls.startEngine()</script>
    </binding>
    <mod-up>
     <binding>
      <command>nasal</command>
      <script>props.setAll("/controls/engines/engine", "starter", 0)</script>
     </binding>
    </mod-up>
  </action>
 </animation>

 <animation>
  <type>select</type>
  <object-name>StarterHandle</object-name>
  <condition>
   <equals>
    <property>engines/engine[0]/cranking</property>
    <value>false</value>
   </equals>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>StarterHandlePulled</object-name>
  <object-name>StarterRope2Pulled</object-name>
  <condition>
   <equals>
    <property>engines/engine[0]/cranking</property>
    <value>true</value>
   </equals>
  </condition>
 </animation>

